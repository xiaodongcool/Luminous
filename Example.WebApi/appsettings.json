{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Luminous": {
    "AspNetCore": {
      "AppendEnumMeaning": true,
    },
    "Log": {
      "MinLevel": "Information",
      "ElasticSearch": {
        "Urls": [
          "http://192.168.0.180:9200"
        ],
        "IndexFormat": "example-web-api-{0:yyyy.MM.dd}"
      },
      "MySql": {
        "ConnectionString": "server=192.168.0.180;port=3306;uid=root;pwd=123;database=test",
        "Table": "logs"
      },
      "File": {
        "File": "file.log",
        "Interval": "Day"
      }
    }
  }
}
